<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE form-validation PUBLIC
     "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.3.0//EN"
     "http://jakarta.apache.org/commons/dtds/validator_1_3_0.dtd">
<form-validation>

    <global>
    </global>

    <formset>

    <form name="loginform">
    	<!-- ID(入力必須) -->
    	<field property="in_id" depends="required">
    		<arg position="0" key="field.in_id" resource="true"/>
    	</field>
    	<!-- PASS(入力必須) -->
    	<field property="in_pass" depends="required">
    		<arg position="0" key="field.in_pass" resource="true"/>

    	</field>

    </form>

    </formset>
</form-validation>